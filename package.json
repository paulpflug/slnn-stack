{
  "name": "slnn-stack",
  "description": "systemd letsencrypt nginx node stack",
  "version": "0.0.2",
  "homepage": "https://github.com/paulpflug/",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/paulpflug/slnn-stack"
  },
  "engines": {
    "node": "*"
  },
  "bin": {
    "slnn": "./cli.js"
  },
  "files": [
    "*.js",
    "lib"
  ],
  "dependencies": {
    "commander": "^2.9.0",
    "fs-extra": "^2.1.2",
    "ip": "^1.1.5",
    "script-runner": "^0.1.6",
    "which": "^1.2.14"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coffeescript": "^2.0.0-alpha1",
    "mocha": "^3.2.0",
    "mocha-watch": "0.1.0",
    "recursive-uncache": "0.1.0"
  },
  "keywords": [
    "systemd",
    "letsencrypt",
    "nginx",
    "node",
    "stack"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "coffee --no-header --compile --output ./lib/ src/*.coffee",
    "watch": "run-npm -p watch:*",
    "watch:coffee": "coffee --no-header --watch --output ./lib/ src/*.coffee",
    "watch:test": "mocha-watch",
    "test": "mocha",
    "preversion": "npm run build && npm run test",
    "version": "git add .",
    "postversion": "git push && git push --tags && npm publish"
  }
}
